<section id="new-session">
  <div class="container">
    <div class="row">

        <h3>New master session</h3>

        <form role="form" method="PUT" action="/sessions/master">
          <fieldset>
            <div class="form-group">
              <input class="form-control" placeholder="Session name" type="text" name="name">
            </div>
            <div class="form-group">
              <input class="form-control" placeholder="Password" type="password" name="password">
            </div>
            <input class="btn btn-lg btn-success btn-block" type="submit" value="Login">
          </fieldset>
        </form>
    </div>
  </div>
</section>
